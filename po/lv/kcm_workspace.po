#, kde-format
msgctxt "part of a sentence: 'Clicking files or folders selects them'"
msgid "Selects them"
msgstr "To atlasa"

#: ui/main.qml:144
#, kde-format
msgid "Open by double-clicking instead"
msgstr "Atver ar dubultklikšķi"

#: ui/main.qml:156
#, kde-format
msgctxt "part of a sentence: 'Clicking files or folders opens them'"
msgid "Opens them"
msgstr "To atver"

#: ui/main.qml:169
#, kde-format
msgid "Select by clicking on item's selection marker"
msgstr "Atlasīt, klikšķinot uz vienuma atlases marķiera"

#: ui/main.qml:184
#, kde-format
