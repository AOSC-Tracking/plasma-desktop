#: ui/main.qml:31
#, kde-format
msgid "Position on screen:"
msgstr "Пазіцыя на экране:"

#: ui/main.qml:35
#, kde-format
msgid "Top"
msgstr "Уверсе"

#: ui/main.qml:42
#, kde-format
msgid "Center"
msgstr "Па цэнтры"

#: ui/main.qml:55
#, kde-format
msgid "Activation:"
msgstr "Актывацыя:"

#: ui/main.qml:58
#, kde-format
msgctxt "@option:check"
msgid "Activate when pressing any key on the desktop"
msgstr "Актывацыя пры націсканнім любой клавішы на працоўным стале"

#: ui/main.qml:71
#, kde-format
msgid "History:"
msgstr "Гісторыя:"

#: ui/main.qml:74
#, kde-format
msgctxt "@option:check"
msgid "Remember past searches"
msgstr "Запамінаць мінулыя пошукі"

#: ui/main.qml:85
#, kde-format
msgctxt "@option:check"
msgid "Retain last search when re-opening"
msgstr "Захоўваць апошні пошук пры паўторным адкрыцці"

#: ui/main.qml:96
#, kde-format
msgctxt "@option:check"
msgid "Activity-aware (last search and history)"
msgstr "Улічванне актыўнасці (апошні пошук і гісторыя)"

#: ui/main.qml:111
#, kde-format
msgid "Clear History"
msgstr "Ачысціць гісторыю"

#: ui/main.qml:112
#, kde-format
msgctxt "@action:button %1 activity name"
msgid "Clear History for Activity \"%1\""
msgstr "Ачысціць гісторыю для пакоя \"%1\""

#: ui/main.qml:113
#, kde-format
msgid "Clear History…"
msgstr "Ачысціць гісторыю…"

#: ui/main.qml:142
#, kde-format
msgctxt "@item:inmenu delete krunner history for all activities"
msgid "For all activities"
msgstr "Для ўсіх пакояў"

#: ui/main.qml:155
#, kde-format
msgctxt "@item:inmenu delete krunner history for this activity"
msgid "For activity \"%1\""
msgstr "Для пакоя \"%1\""

#: ui/main.qml:172
#, kde-format
msgctxt "@label"
msgid "Plugins:"
msgstr "Убудовы:"

#: ui/main.qml:176
#, kde-format
msgctxt "@action:button"
msgid "Configure Enabled Search Plugins…"
msgstr "Наладжванне ўключаных убудоў пошуку…"
